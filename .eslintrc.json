{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "indent": ["error", 4],
        "import/no-extraneous-dependencies": "off",
        "max-len": ["error", { "code": 200 }],
        "object-curly-newline": "off",
        "no-shadow": "off",
        "array-callback-return": "off"
    }
}
